  "scripts": {
    "install:dev": "npm install",
    "start": "cross-env NODE_PATH=src  npm-run-all -p watch-css start-js",
    "build": "npm run build-css && NODE_PATH=src react-scripts build",
    "build-css": "node-sass-chokidar src/App.scss -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/App.scss -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "start-local:dev": "REACT_APP_API_GATEWAY_REGION=ap-northeast-1 REACT_APP_API_GATEWAY_URL=https://ryvtix89f1.execute-api.ap-northeast-1.amazonaws.com/development REACT_APP_AWS_REGION=ap-northeast-1 REACT_APP_USER_POOL_ID=ap-northeast-1_nCI9XCZnj REACT_APP_COGNITO_CLIENT_ID=rfhnk86udjmnqu3g1l370jcru npm start",
    "test": "cross-env NODE_PATH=src jest --coverage --env=jsdom",
    "e2e": " node ./node_modules/selenium-cucumber-js/index.js -f ./test/features/weather-page.feature -s ./test/step-definitions -p ./test/page-objects"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-es2015-destructuring": "^6.23.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.26.0",
    "cross-env": "^5.1.3",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-react-intl": "^2.0.3",
    "enzyme-to-json": "^3.3.1",
    "eslint": "^5.3.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.13.0",
    "jest-html-reporter": "^2.0.0",
    "node-sass": "^4.7.2",
    "react-test-renderer": "^16.2.0",
    "redux-logger": "^3.0.6",
    "sass-loader": "^7.1.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "jsdom",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "setupTestFrameworkScriptFile": "<rootDir>/src/setupTests.js",
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "mjs",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "lint-staged": {
    "*.{js,json,scss}": [
      "prettier --write --print-width 120 --tab-width 4 --single-quote --parser babylon",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
